#:import FadeTransition kivy.uix.screenmanager.FadeTransition
<Screen1>:
    canvas:
        Color:
            rgba: 200/255, 50/255,50/255,1
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [25, 25, 25, 25]
    Label:
        text: "Screen 1"
